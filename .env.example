# API Configuration
# Copy this file to .env and update with your current machine's IP address
# You can find your IP by running: ifconfig | grep "inet " | grep -v 127.0.0.1
EXPO_PUBLIC_API_BASE_URL=http://YOUR_IP_ADDRESS:8080/api

# OpenAI Configuration
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here
