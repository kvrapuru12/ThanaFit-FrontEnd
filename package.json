{
  "name": "thanafit-auth",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android:apk": "eas build --platform android --profile preview",
    "build:android:apk:local": "cd android && ./gradlew assembleRelease",
    "build:prod": "npx eas build --platform android --profile production",
    "build:prod:minor": "VERSION_BUMP=minor npx eas build --platform android --profile production",
    "build:prod:major": "VERSION_BUMP=major npx eas build --platform android --profile production",
    "submit:prod": "npx eas submit --platform android --profile production",
    "release": "npm run build:prod && npm run submit:prod"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.16",
    "axios": "^1.12.2",
    "expo": "~53.0.24",
    "expo-asset": "~11.1.7",
    "expo-auth-session": "~6.2.1",
    "expo-av": "~15.1.7",
    "expo-constants": "^18.0.13",
    "expo-crypto": "~14.1.5",
    "expo-file-system": "~18.1.11",
    "expo-secure-store": "~14.2.4",
    "expo-speech": "~13.1.7",
    "expo-speech-recognition": "^2.1.3",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.1.6",
    "metro": "^0.82.0",
    "nativewind": "^4.0.0",
    "openai": "^5.22.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
